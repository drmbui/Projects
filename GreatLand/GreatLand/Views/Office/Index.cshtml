        }
        else
        {
            string here;
        }

        <br />
        <br />
        @using (Html.BeginForm("Index", "Office"))
        {
            <table>
                <tr>
                    <td>
                        <div class="form-group">
                            @Html.Label("ConfirmationLbl", "Confirmation Number", new { @class = "col-md-2 control-label" })
